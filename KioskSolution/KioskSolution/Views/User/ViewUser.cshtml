
@{
    ViewBag.Title = "ViewUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Scripts.Render("~/bundles/viewusers")

<div class="page-head">
    <h2 class="pull-left"><i class="fa fa-user"></i> User Management</h2>

    <!-- Breadcrumb -->
    <div class="bread-crumb pull-right">
        <a><i class="fa fa-user"></i> User Management</a>
        <!-- Divider -->
        <span class="divider">/</span>
        <a class="bread-current"> View User</a>
    </div>

    <div class="clearfix"></div>

</div>
<div class="matter">
    <div class="container">
        <div class="row">
            <div class="col-md-12">


                <div class="widget wgreen">

                    <div class="widget-head">
                        <div class="pull-left">User List</div>

